urn (\n    <div className='image-selector-container'>\n      <div className='select-image-avatar-container'>\n        <label htmlFor='file-input' className='file-upload'>\n          <AvatarOrInitials\n            {...{\n              url: state.croppedImg,\n              name: createdUserInfo?.LecturerNameInEnglish,\n              className: 'select-picture-avatar',\n              alt: 'avatar',\n            }}\n          />\n          <p>Maximum Size: 2 MB</p>\n        </label>\n\n        <input\n          id='file-input'\n          type='file'\n          accept='image/*'\n          onChange={handleFileChange}\n          className='select-image-input-ref'\n          ref={inputRef}\n        />\n        <div className='user-information select-image-user-info'>\n          <span>\n            {getFullName(createdUserInfo?.LecturerNameInEnglish) || ''}\n          </span>\n          <span>\n            {createdUserInfo?.department?.DepartmentNameInEnglish +\n              ' Department' || ''}\n          </span>\n          <p>{createdUserInfo?.role?.name || 'role'}</p>\n        </div>\n      </div>\n\n      {state.cropperOpen && (\n        <div className='cropper-wrapper'>\n          <AvatarEditor\n            ref={editorRef}\n            image={state.img}\n            width={200}\n            height={200}\n            border={50}\n            color={[255, 255, 255, 0.6]} // RGBA\n            rotate={state.rotate}\n            scale={state.zoom}\n          />\n          <div className='image-edit-tools'>\n            <div className='zoom-slider-container'>\n              <label>Zoom</label>\n              <Slider\n                min={-1}\n                max={10}\n                step={0.1}\n                value={state.zoom}\n                onChange={handleZoomSlider}\n                style={{ width: 200 }}\n              />\n            </div>\n            <div className='zoom-slider-container'>\n              <label>Rotate</label>\n\n              <div className='rotation-buttons-container'>\n                <button onClick={rotateLeft} className='icons8-rotate-left' />\n                <button onClick={rotateRight} className='icons8-rotate-right' />\n              </div>\n            </div>\n            <div className='save-btn-container'>\n              <button onClick={handleCancel}>Cancel</button>\n              <button\n                onClick={handleSave}\n                className='icons8-checkmark'\n              ></button>\n            </div>\n          </div>\n        </div>\n      )}\n    </div>\n  );\n}\n","/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/uploadImage.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/Settings/SettingsPage.js",["409"],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/updateUserAvatar.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/Staff/StaffPage.js",[],{"ruleId":"410","replacedBy":"411"},{"ruleId":"412","replacedBy":"413"},{"ruleId":"414","severity":1,"message":"415","line":15,"column":15,"nodeType":"416","messageId":"417","endLine":15,"endColumn":19},{"ruleId":"410","replacedBy":"418"},{"ruleId":"412","replacedBy":"419"},{"ruleId":"410","replacedBy":"420"},{"ruleId":"412","replacedBy":"421"},{"ruleId":"410","replacedBy":"422"},{"ruleId":"412","replacedBy":"423"},{"ruleId":"410","replacedBy":"424"},{"ruleId":"412","replacedBy":"425"},{"ruleId":"410","replacedBy":"426"},{"ruleId":"412","replacedBy":"427"},{"ruleId":"414","severity":1,"message":"428","line":132,"column":9,"nodeType":"416","messageId":"417","endLine":132,"endColumn":22},{"ruleId":"410","replacedBy":"429"},{"ruleId":"412","replacedBy":"430"},{"ruleId":"410","replacedBy":"431"},{"ruleId":"412","replacedBy":"432"},{"ruleId":"410","replacedBy":"433"},{"ruleId":"412","replacedBy":"434"},{"ruleId":"410","replacedBy":"435"},{"ruleId":"412","replacedBy":"436"},{"ruleId":"410","replacedBy":"437"},{"ruleId":"412","replacedBy":"438"},{"ruleId":"410","replace