n  const [checkedMethod, setCheckedMethod] = useState(null);\n\n  const handleClose = (e, reason) => {\n    if (reason === 'clickaway') {\n      return;\n    }\n\n    setOpenSnackbar(false);\n  };\n  const scrollInto = (ref) => {\n    try {\n      ref.current.scrollIntoView({ behavior: 'smooth' });\n    } catch (e) {\n      console.error(e.message);\n    }\n  };\n\n  const handleChangeMethod = (pathTo) => {\n    if (pathTo) {\n      setCheckedMethod(pathTo);\n      scrollInto(methodRef);\n    }\n  };\n  const fetchUsers = useCallback(\n    ({ users }) => {\n      setCurrentUsersEmails(users.map((u) => u.email));\n    },\n    [setCurrentUsersEmails]\n  );\n\n  if (role?.name !== 'Super Admin') {\n    return <Redirect to={state?.from || '/dashboard'} />;\n  }\n\n  return (\n    <Query query={GET_USERNAMES_EMAILS} onCompletedFunction={fetchUsers}>\n      {({ data: { roles, departments, userInvitations } }) => {\n        return (\n          <main id='add-lecturers-page'>\n            <header className='import-students-header'>\n              <h4 className='font-weight600'>Invite Lecturers and TAs üßë‚Äçüè´</h4>\n\n              <p>\n                Fill the information below to send invitations for the users so\n                that they can register.\n                <br />\n              </p>\n            </header>\n            <CustomizedSnackbars\n              {...{\n                open: openSnackbar,\n                type: snackbarType || 'error',\n                message:\n                  snackbarType === 'success'\n                    ? `Invitations Sent Successfully!`\n                    : 'There was an error sending these invitations, Please try again later!',\n                handleClose,\n              }}\n            />\n            <section className='add-lecturer-method'>\n              <h3>How do you want to send invitations?</h3>\n              <div className='methods-checkbox-container'>\n                <div\n                  className={`method-check-box ${\n                    checkedMethod === 'manually' && 'active-checkbox'\n                  }`}\n                  type='checkbox'\n                  onClick={() => {\n                    handleChangeMethod('manually');\n                  }}\n                >\n                  <span className='checkbox-input' />\n                  <div className='icons8-ball-point-pen' />\n                  <span className='font-weight500'>Manually</span>\n                </div>\n                <div\n                  className={`method-check-box ${\n                    checkedMethod === 'excel-sheet' && 'active-checkbox'\n                  }`}\n                  type='checkbox'\n                  onClick={() => {\n                    handleChangeMethod('excel-sheet');\n                  }}\n                >\n                  <span className='checkbox-input' />\n                  <div className='icons8-check-document' />\n                  <span className='font-weight500'>Excel Sheet</span>\n                </div>\n              </div>\n            </section>\n            <section className='main-content'>\n              <div ref={methodRef}>\n                {checkedMethod === 'manually' && (\n                  <InvitationForm\n                    {...{\n                      roles,\n                      departments,\n                      currentUsersEmail,\n                      userInvitations,\n                      setOpenSnackbar,\n                      setSnackbarType,\n                    }}\n                  />\n                )}\n                {checkedMethod === 'excel-sheet' && (\n                  <DropZone\n                    {...{\n                      departments,\n                      currentUsersEmail,\n                      roles,\n                      userInvitations,\n                      setOpenSnackbar,\n                      setSnackbarType,\n                    }}\n                  />\n                )}\n              </div>\n            </section>\n          </main>\n        );\n      }}\n    </Query>\n  );\n}\n","/Users/3ba2i/Desktop/Graduation Project/client/src/api/queries/getFacultyData.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/login.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Buttons/Button.js",[],["380","381"],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AssignLectures/AssignLecturers.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Modals/AreYouSureModal.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/ImportStudents/ImportStudents.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/types/styles/index.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AdminPanel/SelectCards.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/utlis/apollo/apolloClient.js",[],["382","383"],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AdminPanel/AdminPanel.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/utlis/helpers/handleChangesAndReturnNextState.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/deleteStudent.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/utlis/helpers/calcuateAverage.js",[],["384","385"],"/Users/3ba2i/Desktop/Graduation Project/client/src/redux/reducers/authedReducer.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/redux/reducers/index.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Spinner/spinner.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Charts/DoughnutChart.js",[],["386","387"],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AssignLectures/AssignLecturerToCourse.js",["388","389"],"import { useParams } from 'react-router-dom';\nimport { GET_COURSE_DATA } from 'api/queries/getCourseData';\nimport AvatarOrInitials from 'components/Avatar/AvatarOrInitials';\nimport Query from 'components/Query';\nconst AssignLecturerToCourse = () => {\n  const { courseID } = useParams();\n\n  return (\n    <main id='assign-lecturers-to-courses-page'>\n      <Query query={GET_COURSE_DATA} variables={{ id: courseID }}>\n        {({ data }) => {\n          const {\n            CourseAvatar,\n            CourseNameInEnglish,\n            CourseNameInArabic,\n            academic_years,\n            CourseID,\n            users,\n          } = data?.course;\n          return (\n            <header className='course-assign-header'>\n              <AvatarOrInitials\n                {...{\n                  url: CourseAvatar?.url,\n                  name: CourseNameInEnglish,\n                  alt: 'course-img',\n                  className: 'course-img-avatar',\n                }}\n              />\n              <div>\n                <h6>{CourseNameInEnglish}</h6>\n                <span>\n                  {(academic_years &&\n                    academic_years[0]?.AcademicYearInEnglish) ||\n                    ''}\n                </span>\n                <span>{CourseID}</span>\n              </div>\n            </header>\n          );\n        }}\n      </Query>\n    </main>\n  );\n};\n\nexport default AssignLecturerToCourse;\n",["390","391"],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/createStudent.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/Dashboard/CourseCard.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/queries/getAcademicYears.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Query/index.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Modals/UploadedGroupsModal.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Drawer/DrawerItems.js",["392"],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/Dashboard/DoughnutCard.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/utlis/validation/validation.js",["393"],"export function validateEmail(email) {\n  if (!email) {\n    return false;\n  }\n  if (email.includes('@')) {\n    let emailRegExp = /^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$/g;\n    return emailRegExp.test(email);\n  }\n  let usernameRegExp = /^[a-zA-Z0-9](_(?!(\\.|_))|\\.(?!(_|\\.))|[a-zA-Z0-9]){6,18}[a-zA-Z0-9]$/g;\n  return usernameRegExp.test(email);\n}\nexport function validateEnglishName(name) {\n  let nameInEnglishExp = /^[a-zA-Z\\s]*$/;\n  return nameInEnglishExp.test(name);\n}\nexport function validateArabicName(name) {\n  let nameInArabicExp = /^[\\u0621-\\u064A\\s\\p{N}]+$/;\n  return nameInArabicExp.test(name);\n}\n\nexport function validatePassword(password) {\n  if (!password) return false;\n\n  let passwordRegExp = /^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$/g;\n  return passwordRegExp.test(password);\n}\n",["394","395"],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/common/Logo.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/common/PasswordTextField.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/ErrorPages/404.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/PasswordReset/ForgotPassword.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/forgotPassword.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/resetPasswordMutation.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/common/ErrorIndicator.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/PasswordReset/ResetPassword.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/redux/actions/authedAction.js",[],["396","397"],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/LoginPage/index.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/ErrorPages/502.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Avatar/AvatarOrInitials.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Modals/FormatModal.js",["398"],"import Backdrop from '@material-ui/core/Backdrop';\nimport Fade from '@material-ui/core/Fade';\nimport Modal from '@material-ui/core/Modal';\nimport React from 'react';\nimport excelFileScreenshot from 'assets/excelsheet.png';\nimport { transitionFormatModalStyle } from 'types/styles';\n\nexport default function TransitionsModal({\n  handleOpen,\n  handleClose,\n  open,\n  title,\n  description,\n  exampleImage,\n}) {\n  const classes = transitionFormatModalStyle();\n\n  return (\n    <div>\n      <Modal\n        aria-labelledby='How to format the excel file?'\n        aria-describedby='Excel file formation'\n        className={classes.modal}\n        open={open}\n        onClose={handleClose}\n        closeAfterTransition\n        BackdropComponent={Backdrop}\n        BackdropProps={{\n          timeout: 500,\n        }}\n      >\n        <Fade in={open}>\n          <div className={classes.paper}>\n            <h2 id='transition-modal-title'>{title}</h2>\n            <p id='transition-modal-description'>{description}</p>\n            <div className='excel-sheet-screenshot-container'>\n              {exampleImage}\n            </div>\n          </div>\n        </Fade>\n      </Modal>\n    </div>\n  );\n}\n","/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/createInvitation.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/queries/getInvitationWithEmail.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AddLecturers/InvitationForm.js",["399"],"import { useMutation } from '@apollo/client';\nimport { MenuItem } from '@material-ui/core';\nimport TextField from '@material-ui/core/TextField';\nimport {\n  CREATE_INVITATION,\n  UPDATE_INVITATION_DATE,\n} from 'api/mutations/createInvitation';\nimport React, { useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { handleSubmitInvitationForm } from 'utlis/helpers/invtiationOperations/invitationOperations';\n\nexport function InvitationForm({\n  roles,\n  departments,\n  currentUsersEmail,\n  userInvitations,\n  setOpenSnackbar,\n  setSnackbarType,\n}) {\n  const {\n    authedUser: { LecturerNameInEnglish, id },\n  } = useSelector((state) => state?.authReducer);\n\n  const [email, setEmail] = useState(null);\n  const [selectedRole, setRole] = useState('');\n  const [department, setDepartment] = useState('');\n  const [emailError, setEmailError] = useState(false);\n  const [createInvitation, { loading }] = useMutation(CREATE_INVITATION);\n  const [updateInvitationDate, { loading: updateLoading }] = useMutation(\n    UPDATE_INVITATION_DATE\n  );\n\n  const validateEmail = () => {\n    if (currentUsersEmail.includes(email)) {\n      setEmailError(true);\n    }\n  };\n\n  const onSelectRole = (e) => {\n    setRole(e.target.value);\n  };\n  const onSelectDepartment = (e) => {\n    setDepartment(e.target.value);\n  };\n  const onSubmitInvitationForm = async (e) => {\n    e.preventDefault();\n\n    if (emailError) return;\n    try {\n      await handleSubmitInvitationForm({\n        departments,\n        roles,\n        department,\n        email,\n        selectedRole,\n        userInvitations,\n        userID: id,\n        LecturerNameInEnglish,\n        createInvitation,\n        updateInvitationDate,\n        setOpenSnackbar,\n        setSnackbarType,\n      });\n\n      setSnackbarType('success');\n    } catch (e) {\n      console.error(e.message);\n      setSnackbarType('error');\n    } finally {\n      setOpenSnackbar(true);\n    }\n  };\n  return (\n    <div className='add-lecturer-manually-container'>\n      <header>\n        <h3>Fill the following form and click send</h3>\n      </header>\n      <form\n        className='add-lecturer-form'\n        autoComplete='off'\n        onSubmit={onSubmitInvitationForm}\n      >\n        <TextField\n          required\n          type='email'\n          autoComplete='email'\n          id='outlined-error-helper-text-email'\n          label='Email'\n          helperText={\n            emailError\n              ? 'This email is already associated with an account.'\n              : ''\n          }\n          variant='outlined'\n          error={emailError}\n          onChange={(e) => {\n            setEmailError(false);\n            setEmail(e.target.value);\n          }}\n          onBlur={validateEmail}\n        />\n\n        <TextField\n          id='filled-select-currency'\n          select\n          required\n          label='Role'\n          value={selectedRole}\n          onChange={onSelectRole}\n          variant='outlined'\n          onChange={onSelectRole}\n        >\n          {roles.map(({ id, name }) => {\n            return (\n              ['Lecturer', 'Teacher Assistant'].includes(name) && (\n                <MenuItem key={id} value={id}>\n                  {name}\n                </MenuItem>\n              )\n            );\n          })}\n        </TextField>\n        <TextField\n          id='filled-select-currency'\n          select\n          required\n          label='Department'\n          value={department}\n          onChange={onSelectDepartment}\n          variant='outlined'\n        >\n          {departments.map(({ id, DepartmentNameInEnglish }) => (\n            <MenuItem key={id} value={id}>\n              {DepartmentNameInEnglish}\n            </MenuItem>\n          ))}\n        </TextField>\n        <div className='invitation-btn-container'>\n          <button\n            type='submit'\n            className='invitation-btn'\n            disabled={loading || emailError || updateLoading}\n          >\n            <span>Send Invitation</span>\n          </button>\n        </div>\n      </form>\n    </div>\n  );\n}\n","/Users/3ba2i/Desktop/Graduation Project/client/src/types/constants/redux-constants.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AddLecturers/UsersInviationsDropZone.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Dropzone/Dropzone.js",["400"],"/Users/3ba2i/Desktop/Graduation Project/client/src/utlis/helpers/getFullName.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/utlis/helpers/invtiationOperations/invitationOperations.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AddLecturers/DataTablePreview.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/queries/getUserInvitation.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/RegisterPage/index.js",["401","402","403","404","405","406"],"import { useMutation } from '@apollo/client';\nimport { LinearProgress } from '@material-ui/core';\nimport { UPDATE_USER_AVATAR } from 'api/mutations/updateUserAvatar';\nimport { UPLOAD_IMAGE } from 'api/mutations/uploadImage';\nimport { GET_USER_INVITATION_INFO } from 'api/queries/getUserInvitation';\nimport { Error } from 'components/common/ErrorIndicator';\nimport Logo from 'components/common/Logo';\nimport ImageSelector from 'components/ImageSelector/ImageSelector';\nimport Query from 'components/Query';\nimport { useEffect, useState } from 'react';\nimport { useParams } from 'react-router';\nimport { Link } from 'react-router-dom';\nimport './register.css';\nimport { SignupForm } from './SignUpForm';\n\nconst RegisterPage = () => {\n  let { token } = useParams();\n  const [tokenInfo, setTokenInfo] = useState({});\n  const [validToken, setValidToken] = useState(true);\n  const [startAnimation, setStartAnimation] = useState(0);\n  const [createdUserInfo, setCreatedUserInfo] = useState(null);\n\n  const onFetchRegistrationToken = ({ userInvitation }) => {\n    setTokenInfo(userInvitation);\n  };\n\n  const handleNextStep = (stepNumber) => {\n    setStartAnimation(stepNumber);\n  };\n\n  if (!token) {\n    return <InvalidInvitation />;\n  }\n\n  return (\n    <main id='registration-page'>\n      <Query\n        query={GET_USER_INVITATION_INFO}\n        variables={{ id: token?.split('token=')[1] }}\n        onCompletedFunction={onFetchRegistrationToken}\n        errorComponent={<InvalidInvitation />}\n      >\n        {({\n          data: {\n            userInvitation: {\n              id,\n              department,\n              role,\n              email,\n              isUsed,\n              latest_invitation_time,\n            },\n            users,\n          },\n        }) => {\n          console.log(new Date() - new Date(latest_invitation_time));\n\n          if (\n            (new Date() - new Date(latest_invitation_time)) / (1000 * 1000) >=\n            2592\n          ) {\n            return (\n              <section className='expired-invitation'>\n                <h1>‚åõÔ∏è</h1>\n                <h3>Expired Invitation</h3>\n                <p>\n                  This invitation has been expired, please{' '}\n                  <a href='mailto:attend.qrsys@gmail.com'>contact us</a> to send\n                  you a new one.\n                </p>\n              </section>\n            );\n          }\n          if (isUsed) {\n            return (\n              <section className='expired-invitation'>\n                <h1>‚úîÔ∏è</h1>\n                <h3>Already Used Invitation</h3>\n                <p>\n                  This invitation has been already used, if you have problem\n                  logging in to your account you can always{' '}\n                  <Link to='/forgot-password'>reset your password</Link>\n                </p>\n              </section>\n            );\n          }\n          return (\n            <>\n              <Logo className='small-logo' />\n              <section className='register-page-container'>\n                <SignupForm\n                  existingUsernames={users.map((user) =>\n                    user.username.toLowerCase()\n                  )}\n                  {...{\n                    email,\n                    department: department,\n                    role: role,\n                    invitationID: id,\n                    handleNextStep,\n                    startAnimation,\n                    setCreatedUserInfo,\n                  }}\n                />\n                <section\n                  className={`avatar-upload-step ${\n                    startAnimation === 1 && 'animation-begin-next-stage'\n                  } ${startAnimation > 1 && 'animation-begin-previous-stage'}`}\n                >\n                  {createdUserInfo ? (\n                    <UploadImageStep\n                      createdUserInfo={createdUserInfo}\n                      handleNextStep={handleNextStep}\n                    />\n                  ) : (\n                    <section className='expired-invitation'>\n                      <h1>‚ùå</h1>\n                      <h3>Not a Valid Process</h3>\n                      <p>\n                        Please go back to fill the information required to\n                        create your account.\n                      </p>\n                      <button\n                        className='go-back-btn'\n                        onClick={() => {\n                          handleNextStep(0);\n                        }}\n                      >\n                        <span>Go Back</span>\n                        <span className='icons8-back'></span>\n                      </button>\n                    </section>\n                  )}\n                </section>\n                <LastStage startAnimation={startAnimation} />\n              </section>\n            </>\n          );\n        }}\n      </Query>\n    </main>\n  );\n};\n\nexport default RegisterPage;\n\nconst InvalidInvitation = () => {\n  return (\n    <section className='expired-invitation'>\n      <h1>‚ùå</h1>\n      <h3>Invitation Not Found</h3>\n      <p>\n        We can't find your invitation in our system, please make sure this\n        invitation is valid or{' '}\n        <a href='mailto:attend.qrsys@gmail.com'>contact us</a> to send you a new\n        one.\n      </p>\n    </section>\n  );\n};\n\nfunction LastStage({ startAnimation }) {\n  return (\n    <section\n      className={`final-register-stage ${\n        startAnimation === 2 && 'final-register-stage-begin-animation'\n      }`}\n    >\n      <header>\n        <h1>üéâ</h1>\n        <span className='bold-text-label'>\n          Congratulations, Your account was created successfully.\n          <br />\n          You can now go back to login page.\n        </span>\n      </header>\n      <Link style={{ width: '100%' }} to='/login'>\n        <button\n          type='submit'\n          className='register-submit-btn'\n          style={{ width: '100%' }}\n        >\n          <span>Go Back to Login</span>\n          <span className='icons8-right' />\n        </button>\n      </Link>\n    </section>\n  );\n}\n\nfunction UploadImageStep({ createdUserInfo, handleNextStep }) {\n  const [currentImageState, setCurrentImageState] = useState(null);\n  const [errorMessage, setErrorMessage] = useState(null);\n  const [uploadImageMutation, { loading }] = useMutation(UPLOAD_IMAGE);\n  const [updateUserAvatar, { loading: updateAvatarLoading }] = useMutation(\n    UPDATE_USER_AVATAR\n  );\n\n  const onUploadImage = async () => {\n    try {\n      const imageFile = dataURLtoFile(\n        currentImageState.croppedImg,\n        'profile-avatar'\n      );\n\n      if (imageFile) {\n        if (imageFile.size > 2097152) {\n          setErrorMessage('Image is too big, please select a smaller image.');\n          return;\n        }\n        if (\n          !/([a-zA-Z0-9\\s_\\\\.\\-\\(\\):])+(.png|.jpg|.jpeg|.tiff|.webp|)$/i.test(\n            imageFile.type\n          )\n        ) {\n          setErrorMessage('Not a supported image type!');\n          return;\n        }\n        const {\n          data: {\n            upload: { id },\n          },\n        } = await uploadImageMutation({\n          variables: {\n            file: imageFile,\n          },\n        });\n\n        await updateUserAvatar({\n          variables: {\n            id: createdUserInfo.id,\n            avatar: id,\n          },\n        });\n        console.log('DONEEEE');\n        handleNextOrSkip(2);\n      }\n    } catch (err) {\n      console.error(err.message);\n    }\n  };\n\n  const ignoreError = () => {\n    setErrorMessage(null);\n  };\n\n  const handleNextOrSkip = () => {\n    handleNextStep(2);\n  };\n  return (\n    <>\n      {(loading || updateAvatarLoading) && (\n        <LinearProgress className='loading-bar' />\n      )}\n      <div\n        className={`${(loading || updateAvatarLoading) && 'disabled-form'}`}\n      />\n      <div className='image-uploader-step'>\n        <header>\n          <h5 className='create-account-title'>Select a Profile Picture</h5>\n          <div\n            style={{\n              marginBottom: '1rem',\n              marginTop: '1rem',\n              marginLeft: '1rem',\n            }}\n          />\n        </header>\n        {errorMessage && <Error {...{ message: errorMessage, ignoreError }} />}\n\n        <ImageSelector\n          createdUserInfo={createdUserInfo}\n          setCurrentImageState={setCurrentImageState}\n        />\n        <div className='upload-avatar-btns'>\n          <button\n            disabled={loading || updateAvatarLoading}\n            onClick={handleNextOrSkip}\n          >\n            Skip\n          </button>\n          <button\n            onClick={onUploadImage}\n            disabled={loading || updateAvatarLoading}\n          >\n            Set Profile Picture\n          </button>\n        </div>\n      </div>\n    </>\n  );\n}\n\nfunction dataURLtoFile(dataurl, filename) {\n  var arr = dataurl.split(','),\n    mime = arr[0].match(/:(.*?);/)[1],\n    bstr = atob(arr[1]),\n    n = bstr.length,\n    u8arr = new Uint8Array(n);\n  while (n--) {\n    u8arr[n] = bstr.charCodeAt(n);\n  }\n  return new File([u8arr], filename, { type: mime });\n}\n","/Users/3ba2i/Desktop/Graduation Project/client/src/App.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/