ateArabicName(name) {\n  let nameInArabicExp = /^[\\u0621-\\u064A\\s\\p{N}]+$/;\n  return nameInArabicExp.test(name);\n}\n\nexport function validatePassword(password) {\n  if (!password) return false;\n\n  let passwordRegExp = /^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$/g;\n  return passwordRegExp.test(password);\n}\n",["437","438"],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/common/Logo.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/common/PasswordTextField.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/ErrorPages/404.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/PasswordReset/ForgotPassword.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/forgotPassword.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/resetPasswordMutation.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/common/ErrorIndicator.js",[],["439","440"],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/PasswordReset/ResetPassword.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/redux/actions/authedAction.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/LoginPage/index.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/ErrorPages/502.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Avatar/AvatarOrInitials.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Modals/FormatModal.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/createInvitation.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/queries/getInvitationWithEmail.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AddLecturers/InvitationForm.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/types/constants/redux-constants.js",[],["441","442"],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AddLecturers/UsersInviationsDropZone.js",[],