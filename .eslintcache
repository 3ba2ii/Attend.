erts/Alerts';\nimport Query from 'components/Query';\nimport React, { useCallback, useRef, useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { Redirect, useLocation } from 'react-router-dom';\nimport './add_lecturers.css';\nimport { DropZone } from './UsersInviationsDropZone';\nimport { InvitationForm } from './InvitationForm';\nimport './styled-table.css';\n\nexport default function AddLecturersPage() {\n  const {\n    authedUser: { LecturerNameInEnglish, role, id },\n  } = useSelector((state) => state?.authReducer);\n\n  const { state } = useLocation();\n  const [currentUsersEmail, setCurrentUsersEmails] = useState([]);\n  const methodRef = useRef(null);\n  const [openSnackbar, setOpenSnackbar] = useState(false);\n  const [snackbarType, setSnackbarType] = useState('');\n  const [checkedMethod, setCheckedMethod] = useState(null);\n\n  const handleClose = (e, reason) => {\n    if (reason === 'clickaway') {\n      return;\n    }\n\n    setOpenSnackbar(false);\n  };\n  const scrollInto = (ref) => {\n    try {\n      ref.current.scrollIntoView({ behavior: 'smooth' });\n    } catch (e) {\n      console.error(e.message);\n    }\n  };\n\n  const handleChangeMethod = (pathTo) => {\n    if (pathTo) {\n      setCheckedMethod(pathTo);\n      scrollInto(methodRef);\n    }\n  };\n  const fetchUsers = useCallback(\n    ({ users }) => {\n      setCurrentUsersEmails(users.map((u) => u.email));\n    },\n    [setCurrentUsersEmails]\n  );\n\n  if (role?.name !== 'Super Admin') {\n    return <Redirect to={state?.from || '/dashboard'} />;\n  }\n\n  return (\n    <Query query={GET_USERNAMES_EMAILS} onCompletedFunction={fetchUsers}>\n      {({ data: { roles, departments, userInvitations } }) => {\n        return (\n          <main id='add-lecturers-page'>\n            <header className='import-students-header'>\n              <h4 className='font-weight600'>Invite Lecturers and TAs 🧑‍🏫</h4>\n\n              <p>\n                Fill the information below to send invitations for the users so\n                that they can register.\n                <br />\n              </p>\n            </header>\n            <CustomizedSnackbars\n              {...{\n                open: openSnackbar,\n                type: snackbarType || 'error',\n                message:\n                  snackbarType === 'success'\n                    ? `Invitations Sent Successfully!`\n                    : 'There was an error sending these invitations, Please try again later!',\n                handleClose,\n              }}\n            />\n            <section className='add-lecturer-method'>\n              <h3>How do you want to send invitations?</h3>\n              <div className='methods-checkbox-container'>\n                <div\n                  className={`method-check-box ${\n                    checkedMethod === 'manually' && 'active-checkbox'\n                  }`}\n                  type='checkbox'\n                  onClick={() => {\n                    handleChangeMethod('manually');\n                  }}\n                >\n                  <span className='checkbox-input' />\n                  <div className='icons8-ball-point-pen' />\n                  <span className='font-weight500'>Manually</span>\n                </div>\n                <div\n                  className={`method-check-box ${\n                    checkedMethod === 'excel-sheet' && 'active-checkbox'\n                  }`}\n                  type='checkbox'\n                  onClick={() => {\n                    handleChangeMethod('excel-sheet');\n                  }}\n                >\n                  <span className='checkbox-input' />\n                  <div className='icons8-check-document' />\n                  <span className='font-weight500'>Excel Sheet</span>\n                </div>\n              </div>\n            </section>\n            <section className='main-content'>\n              <div ref={methodRef}>\n                {checkedMethod === 'manually' && (\n                  <InvitationForm\n                    {...{\n                      roles,\n                      departments,\n                      currentUsersEmail,\n                      userInvitations,\n                      setOpenSnackbar,\n                      setSnackbarType,\n                    }}\n                  />\n                )}\n                {checkedMethod === 'excel-sheet' && (\n                  <DropZone\n                    {...{\n                      departments,\n                      currentUsersEmail,\n                      roles,\n                      userInvitations,\n                      setOpenSnackbar,\n                      setSnackbarType,\n                    }}\n                  />\n                )}\n              </div>\n            </section>\n          </main>\n        );\n      }}\n    </Query>\n  );\n}\n",["516","517"],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/queries/getFacultyData.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/login.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AssignLectures/AssignLecturers.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/components/Modals/AreYouSureModal.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/ImportStudents/ImportStudents.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/types/styles/index.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AdminPanel/SelectCards.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/utlis/apollo/apolloClient.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/pages/DataEntry/AdminPanel/AdminPanel.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/utlis/helpers/handleChangesAndReturnNextState.js",[],"/Users/3ba2i/Desktop/Graduation Project/client/src/api/mutations/deleteStudent.js",[],["518","519"],"/Users/3ba2i/Desktop/Graduation Project/client/src/utlis/helpers